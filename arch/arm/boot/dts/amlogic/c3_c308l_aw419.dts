/dts-v1/;

#include "meson-c3.dtsi"
#include "meson-c3_skt-panel.dtsi"

/ {
	amlogic-dt-id = "c3_c308l_aw419";
	compatible = "c3_c308l_aw419";

	aliases {
		serial0 = &uart_B;
		serial1 = &uart_E;
		serial2 = &uart_A;
		serial3 = &uart_C;
		serial4 = &uart_D;
		serial5 = &uart_F;
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		spi0 = &spifc;
		spi1 = &spicc0;
		spi2 = &spicc1;
		tsensor0 = &tsensor;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory@0 {
		device_type = "memory";
		linux,usable-memory = <0x0 0x80000000>;
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		secmon_reserved:linux,secmon {
			compatible = "shared-dma-pool";
			no-map;
			size = <0x900000>;
			alignment = <0x100000>;
			reg = <0x07F00000 0x900000>;
		};
		nna_cma_reserved: linux,nna_cma {
			compatible = "shared-dma-pool";
			//alloc-ranges = <0x70000000 0x18000000>;
			size = <0x2000000>;
			alignment = <0x400000>;
			reusable;
			//no-map;
			status = "okay";
		};

		logo_reserved:linux,meson-fb {
			compatible = "shared-dma-pool";
			reusable;
			/* keep sync with fb */
			/* fox example:logo_addr = "0x3f800000"*/
			size = <0x1800000>;
			alignment = <0x400000>;
			reg = <0x7e800000 0x1800000>;
		};

		ion_cma_reserved:linux,ion-dev {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x3000000>;
			alignment = <0x400000>;
		};

		rtos_reserved:linux,rtos_reserved {
			compatible = "amlogic, aml_rtos_memory";
			status = "okay";
			reg = <0x09000000 0x06000000>;
			no-map;
		};

		dsp_fw_reserved:linux,dsp_fw {
			compatible = "shared-dma-pool";
			reusable;
			alignment = <0x400000>;
			reg = <0x3400000 0x0>;
			status = "disabled";
		};

		ramoops@0x08800000 {
			compatible = "ramoops";
			reg = <0x08800000 0x00100000>;
			record-size = <0x20000>;
			console-size = <0x20000>;
			ftrace-size = <0x80000>;
			pmsg-size = <0x0>;
			no-map;
		};

		debug_reserved:linux,iotrace {
			compatible = "amlogic, iotrace";
			reg = <0x04f00000 0x00100000>;
			io-size = <0x1b000>;
			sched-size = <0x2000>;
			irq-size = <0x1000>;
			smc-size = <0x1000>;
			misc-size = <0x1000>;
		};

		pmz_cma_a1_reserved:anonymous {
			compatible = "shared-dma-pool";
			//reusable;
			no-map;
			size = <0x10000000>;
			alignment = <0x400000>;
			status = "okay";
		};

		pmz_cma_a2_reserved:anonymous_ext{
			compatible = "shared-dma-pool";
			//reusable;
			no-map;
			size = <0x6000000>;
			alignment = <0x400000>;
			status = "disabled";
		};

		codec_mm_cma:linux,codec_mm_cma {
			compatible = "shared-dma-pool";
			reusable;
			/* ion_codec_mm max can alloc size 80M*/
			size = <0x2400000>;
			alignment = <0x400000>;
			linux,contiguous-region;
		};
		/* codec shared reserved */
		codec_mm_reserved:linux,codec_mm_reserved {
			compatible = "amlogic, codec-mm-reserved";
			size = <0x0>;
			alignment = <0x100000>;
			//no-map;
		};
		jpegenc_cma_reserved:linux,jpegenc_cma {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x0>;
			alignment = <0x400000>;
			status = "disabled";
		};

		/* global autoconfigured region for contiguous allocations */
		linux,cma {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x800000>;
			alignment = <0x400000>;
			linux,cma-default;
			alloc-ranges = <0x0 0x30000000>;
		};
	};

	codec_mm {
		compatible = "amlogic, codec, mm";
		memory-region = <&codec_mm_cma &codec_mm_reserved>;
		dev_name = "codec_mm";
		status = "okay";
	};

	sdcard_power: regulator-sdcard {
		compatible = "regulator-fixed";
		regulator-name = "SDCARD_POWER";
		vin-supply = <&vddao3v3_reg>;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		//gpio = <&gpio GPIOD_13 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
	};

	vddio_card: gpio-regulator {
		compatible = "regulator-gpio";
		regulator-name = "VDDIO_CARD";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
		//gpio = <&gpio GPIOE_2 GPIO_ACTIVE_HIGH>;
		gpios-states = <1>;

		/* Based on P200 schematics, signal CARD_1.8V/3.3V_CTR */
		states = <1800000 1
			  3300000 0>;
	};

	pmz_a1_dev {
		compatible = "amlogic, pmz_dev";
		memory-region = <&pmz_cma_a1_reserved>;
		pmz-allocator = "priv";
	};

	pmz_a2_dev {
		compatible = "amlogic, pmz_dev";
		memory-region = <&pmz_cma_a2_reserved>;
		pmz-allocator = "priv";
	};

        hantro {
            compatible = "hantro";
            status = "okay";
            interrupts = <0x00 0x86 0x01 0x00 0x87 0x01 0x00 0x88 0x01>;
            interrupt-names = "vc9000e_irq0\0vc9000e_irq1\0vc9000e_irq1";
        };


	/* Audio Related start */
	dummy_codec:dummy_codec{
		#sound-dai-cells = <0>;
		compatible = "amlogic, aml_dummy_codec";
		status = "okay";
	};

	acodec:acodec {
		#sound-dai-cells = <0>;
		compatible = "amlogic, t3_acodec";
		status = "okay";
		reg = <0xFE01A000 0x20>;
		tdmout_index = <0>;
		tdmin_index = <0>;
		dat0_ch_sel = <0>;
		reset-names = "acodec";
		resets = <&reset RESET_ACODEC>;
		diff_input = <1>;
		//power-domains = <&pwrdm PDID_CX_ACODEC>;
	};

	auge_sound {
		compatible = "amlogic, auge-sound-card";
		aml-audio-card,name = "AML-AUGESOUND";

		/*avout mute gpio*/
		//avout_mute-gpios = <&gpio GPIOD_0 GPIO_ACTIVE_HIGH>;

		aml-audio-card,dai-link@0 {
			format = "i2s"; //"dsp_a";
			mclk-fs = <256>;
			//continuous-clock;
			//bitclock-inversion;
			//frame-inversion;
			/* master mode */
			bitclock-master = <&tdma>;
			frame-master = <&tdma>;
			/* slave mode */
			/*
			 * bitclock-master = <&tdmacodec>;
			 * frame-master = <&tdmacodec>;
			 */
			suffix-name = "alsaPORT-pcm";
			tdmacpu: cpu {
				sound-dai = <&tdma>;
				dai-tdm-slot-tx-mask = <1 1>;
				dai-tdm-slot-rx-mask = <1 1>;
				dai-tdm-slot-num = <2>;
				dai-tdm-slot-width = <32>;
				system-clock-frequency = <12288000>;
			};
			tdmacodec: codec {
				sound-dai = <&dummy_codec &acodec>;
			};
		};

		aml-audio-card,dai-link@1 {
			format = "i2s";
			mclk-fs = <256>;
			//continuous-clock;
			//bitclock-inversion;
			//frame-inversion;
			/* master mode */
			bitclock-master = <&tdmb>;
			frame-master = <&tdmb>;
			/* slave mode */
			//bitclock-master = <&tdmbcodec>;
			//frame-master = <&tdmbcodec>;
			/* suffix-name, sync with android audio hal
			 * what's the dai link used for
			 */
			suffix-name = "alsaPORT-i2s";
			tdmbcpu: cpu {
				sound-dai = <&tdmb>;
				dai-tdm-slot-tx-mask = <1 1>;
				dai-tdm-slot-rx-mask = <1 1>;
				dai-tdm-slot-num = <2>;
				dai-tdm-slot-width = <32>;
				system-clock-frequency = <12288000>;
			};
			tdmbcodec: codec {
				sound-dai = <&dummy_codec>;
			};
		};

		aml-audio-card,dai-link@2 {
			mclk-fs = <64>;
			/* suffix-name, sync with android audio hal
			 * what's the dai link used for
			 */
			suffix-name = "alsaPORT-pdm";
			cpu {
				sound-dai = <&pdm>;
			};
			codec {
				sound-dai = <&dummy_codec>;
			};
		};

		aml-audio-card,dai-link@3 {
			mclk-fs = <256>;
			continuous-clock;
			suffix-name = "alsaPORT-loopback";
			cpu {
				sound-dai = <&loopback>;
				system-clock-frequency = <12288000>;
			};
			codec {
				sound-dai = <&dummy_codec>;
			};
		};
	};

	picdec {
		compatible = "amlogic, picdec";
		status = "okay";
	};

	audiolocker: locker {
		compatible = "amlogic, audiolocker";
		clocks = <&clkaudio CLKID_AUDIO_LOCKER_OUT
				&clkaudio CLKID_AUDIO_LOCKER_IN
				&clkaudio CLKID_AUDIO_MCLK_C
				&clkaudio CLKID_AUDIO_MCLK_D
				&clkc CLKID_FCLK_DIV2
				&clkc CLKID_FCLK_DIV3>;
		clock-names = "lock_out", "lock_in", "out_src",
					"in_src", "out_calc", "in_ref";
		interrupts = <GIC_SPI 42 IRQ_TYPE_EDGE_RISING>;
		interrupt-names = "irq";
		frequency = <49000000>; /* pll */
		dividor = <49>; /* locker's parent */
		//power-domains = <&pwrdm PDID_CX_AUDIO>;
		status = "disabled";
	};
	/* Audio Related end */
	/* to check
	 *wifivbat_power: regulator-wifivbat {
	 *	compatible = "regulator-fixed";
	 *	regulator-name = "WIFIVBAT_POWER";
	 *	vin-supply = <&vddao3v3_reg>;
	 *	regulator-min-microvolt = <3300000>;
	 *	regulator-max-microvolt = <3300000>;
	 *	gpio = <&gpio GPIOD_13 GPIO_ACTIVE_HIGH>;
	 *	enable-active-high;
	 *	regulator-boot-on;
	 *	regulator-always-on;
	 *};

	 *vddio_wf: regulator_wfio {
	 *	compatible = "regulator-fixed";
	 *	regulator-name = "VDDIO_WF";
	 *	vin-supply = <&vddao1v8_reg>;
	 *	regulator-min-microvolt = <1800000>;
	 *	regulator-max-microvolt = <1800000>;
	 *	regulator-boot-on;
	 *	regulator-always-on;
	 *};
	 */

	gpio_keypad{
		compatible = "amlogic, gpio_keypad";
		status = "disabled";
		scan_period = <20>;
		key_num = <1>;
		key_name = "power";
		key_code = <116>;
		key-gpios = <&gpio  GPIOD_4  GPIO_ACTIVE_HIGH>;
		detect_mode = <0>;/*0:polling mode, 1:irq mode*/
	};

	unifykey{
		compatible = "amlogic,unifykey";
		status = "okay";
		unifykey-num = <22>;
		unifykey-index-0 = <&keysn_0>;
		unifykey-index-1 = <&keysn_1>;
		unifykey-index-2 = <&keysn_2>;
		unifykey-index-3 = <&keysn_3>;
		unifykey-index-4 = <&keysn_4>;
		unifykey-index-5 = <&keysn_5>;
		unifykey-index-6 = <&keysn_6>;
		unifykey-index-7 = <&keysn_7>;
		unifykey-index-8 = <&keysn_8>;
		unifykey-index-9 = <&keysn_9>;
		unifykey-index-10= <&keysn_10>;
		unifykey-index-11= <&keysn_11>;
		unifykey-index-12= <&keysn_12>;
		unifykey-index-13= <&keysn_13>;
		unifykey-index-14= <&keysn_14>;
		unifykey-index-15= <&keysn_15>;
		unifykey-index-16= <&keysn_16>;
		unifykey-index-17= <&keysn_17>;
		unifykey-index-18= <&keysn_18>;
		unifykey-index-19= <&keysn_19>;
		unifykey-index-20= <&keysn_20>;
		unifykey-index-21= <&keysn_21>;

		keysn_0: key_0{
			key-name = "usid";
			key-device = "normal";
			key-permit = "read","write","del";
		};
		keysn_1:key_1{
			key-name = "mac";
			key-device = "normal";
			key-permit = "read","write","del";
		};
		keysn_2:key_2{
			key-name = "hdcp";
			key-device = "secure";
