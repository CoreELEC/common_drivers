CONFIG_AMLOGIC_DEBUG_RELABEL=y
CONFIG_AMLOGIC_DEBUG_RELABEL_GKI=n

# early console
CONFIG_AMLOGIC_SERIAL_EARLY_CONSOLE=y

# usb xxx ko
